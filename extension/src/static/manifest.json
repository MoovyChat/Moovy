{
  "short_name": "Moovy Chat",
  "name": "Moovy Chat",
  "description": "Enjoy the Streaming now with the Comments",
  "version": "1.0.2",
  "manifest_version": 3,
  "key": "",
  "permissions": ["identity", "storage", "tabs", "scripting", "system.display"],
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*",
        "*://moovychat.com/*",
        "*://localhost/*"
      ],
      "js": ["index.js", "netflix.js", "record.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "netflix.js",
        "Moovy/moovyIcon.png",
        "Moovy/moovy.png",
        "Moovy/moovy-black-text.svg",
        "Moovy/moovy-black.svg",
        "Moovy/moovy-logo-black.jpg",
        "Moovy/moovy-logo-black.png",
        "Moovy/moovy-logo-white.jpg",
        "Moovy/moovy-text-logo-white.png",
        "Moovy/moovy-text-logo-black.png",
        "bmc.png",
        "patreon-logo.png",
        "patreon-word.png"
      ],
      "matches": [
        "https://www.netflix.com/*",
        "*://moovychat.com/*",
        "*://localhost/*"
      ],
      "use_dynamic_url": true
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://www.netflix.com/*",
      "*://moovychat.com/*",
      "*://localhost/*"
    ]
  },
  "host_permissions": [
    "https://www.netflix.com/*",
    "*://moovychat.com/*",
    "*://localhost/*"
  ],
  "icons": {
    "16": "Moovy/moovyIcon.png",
    "48": "Moovy/moovyIcon.png",
    "128": "Moovy/moovyIcon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Quiet Chat",
    "default_icons": {
      "16": "Moovy/moovyIcon.png",
      "48": "Moovy/moovyIcon.png",
      "128": "Moovy/moovyIcon.png"
    }
  },
  "oauth2": {
    "client_id": "596322814794-o62innt0hvn2ua8mmiksiis0gl4k3279.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "background": {
    "service_worker": "background.js"
  }
}
